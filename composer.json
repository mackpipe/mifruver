{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/backup_migrate": "^5.0@RC",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/color_field": "^2.4",
        "drupal/colorbox": "^1.7",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_update": "^1.7",
        "drupal/console": "^1.9",
        "drupal/context": "^4.0@beta",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/cshs": "^2.3",
        "drupal/ctools": "^3.5",
        "drupal/devel": "3.x-dev",
        "drupal/draggableviews": "^2.0",
        "drupal/ds": "^3.12",
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/extlink": "^1.6",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.1",
        "drupal/fivestar": "^1.0@alpha",
        "drupal/fontawesome": "^2.18",
        "drupal/formblock": "^2.0@beta",
        "drupal/geofield_map": "^2.68",
        "drupal/google_analytics": "^3.1",
        "drupal/google_map_field": "^1.14",
        "drupal/google_tag": "^1.4",
        "drupal/image_widget_crop": "^2.3",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/mailsystem": "^4.3",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_per_role": "^1.3",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panelizer": "^4.4",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/profile": "^1.2",
        "drupal/restui": "^1.20",
        "drupal/slick": "^2.3",
        "drupal/slick_extras": "^1.0@RC",
        "drupal/slick_paragraphs": "^2.1",
        "drupal/slick_views": "^2.4",
        "drupal/svg_image": "^1.14",
        "drupal/token": "^1.9",
        "drupal/transliterate_filenames": "^1.5",
        "drupal/unique_field_ajax": "^2.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_slideshow": "^4.8",
        "drupal/views_templates": "^1.1",
        "drupal/votingapi": "^3.0@beta",
        "drupal/we_megamenu": "^1.11",
        "drupal/webform": "^6.0",
        "drush/drush": "^10.5",
        "vlucas/phpdotenv": "^5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {        
        "files": ["load.environment.php"]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
